    ],
    "client": [
      "io.github.cdagaming.fabric.client.CrystalOptimizerFabricClient"
    ]
  },
  "mixins": ["crystaloptimizer.mixins.json"],
  "depends": {
    "fabricloader": ">=0.15.8",
    "minecraft": ">=1.20 <=1.20.4",
